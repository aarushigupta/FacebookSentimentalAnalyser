{
	"manifest_version": 2,
	"name": "Facebook Sentimenal Analyser",
	"version":	"0.1",
	"browser_action": {
  		"default_icon": "icon.png"
	},
	"icons": { 
		"16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
	"content_scripts": [{
			"matches": ["https://www.facebook.com/*"],
	   		"js": ["jQuery-2.2.4-mini.js", "Sentimental.js", "content.js"]
		}]
}